<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="JRecyclerView">
        <attr name="layoutMode">
            <enum name="LinearHorizontal" value="0"/>
            <enum name="LinearVertical" value="1"/>
            <enum name="Grid" value="2"/>
        </attr>
        <attr name="spanCount"/>
        <attr name="startSpacing" format="dimension"/>
        <attr name="endSpacing" format="dimension"/>
        <attr name="horizontalSpacing" format="dimension"/>
        <attr name="verticalSpacing" format="dimension"/>

    </declare-styleable>
</resources>